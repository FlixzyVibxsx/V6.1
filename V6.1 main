function sandbox(var,func) local env = getfenv(func) local newenv = setmetatable({},{ __index = function(self,k) if k=="script" then return var else return env[k] end end, }) setfenv(func,newenv) return func end
cors = {}
local TweenService = game:GetService("TweenService")
local RunService = game:GetService("RunService")
local InputService = game:GetService("UserInputService")
local PARENT = game:GetService("Players").LocalPlayer:FindFirstChild('PlayerGui')
if not game:GetService("RunService"):IsStudio() then
PARENT = game:GetService("CoreGui")
end
for i,v in next, PARENT:GetChildren() do
if v.Name == "Library" then v:Destroy() end
end
function UILibrary.Main(PrjName,HideKey)
local Library = Instance.new("ScreenGui")
local Script5 = Instance.new("Script")
local Main = Instance.new("Frame")
local HideMain = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local LeftPart = Instance.new("Frame")
local UICorner_2 = Instance.new("UICorner")
local ProjectName = Instance.new("TextLabel")
local Line = Instance.new("Frame")
local Line_2 = Instance.new("Frame")
local Line_3 = Instance.new("Frame")
local ButtonsTab = Instance.new("Frame")
local List = Instance.new("ScrollingFrame")
local UIListLayout = Instance.new("UIListLayout")
local Ignore = Instance.new("Frame")
local UICorner_3 = Instance.new("UICorner")
local Pages = Instance.new("Frame")
local UIPageLayout = Instance.new("UIPageLayout")
--Properties:
Library.Name = "Library"
Library.Parent = PARENT
Library.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
Main.Name = "Main"
Main.Parent = Library
Main.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
Main.BorderSizePixel = 0
Main.ClipsDescendants = true
Main.Position = UDim2.new(0.25, 0, 0.25, 0)
Main.Size = UDim2.new(0,725,0,450)
Main.Visible = false
HideMain.Name = "Main"
HideMain.Parent = Main
HideMain.BackgroundColor3 = Color3.fromRGB(0,195,195)
HideMain.BorderSizePixel = 0
HideMain.ClipsDescendants = true
HideMain.Position = UDim2.new(0, 0, 0, 0)
HideMain.Size = UDim2.new(1, 0, 1, 0)
HideMain.ZIndex = 100
TweenService:Create(HideMain,TweenInfo.new(0.5),{BackgroundTransparency = 0}):Play()
wait(0.5)
TweenService:Create(HideMain,TweenInfo.new(0.3),{BackgroundTransparency = 1}):Play()
Main.Visible = true
UICorner.Parent = Main
UICorner.CornerRadius = UDim.new(0,8)
local FadeBackgroundFrame = Instance.new("Frame")
FadeBackgroundFrame.Name = "FadeBackgroundFrame"
FadeBackgroundFrame.Parent = Main
FadeBackgroundFrame.BackgroundColor3 = Color3.fromRGB(27,27, 27)
FadeBackgroundFrame.BackgroundTransparency = 1.000
FadeBackgroundFrame.BorderSizePixel = 0
FadeBackgroundFrame.Size = UDim2.new(1, 0, 1, 0)
FadeBackgroundFrame.ZIndex = 3
LeftPart.Name = "LeftPart"
LeftPart.Parent = Main
LeftPart.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
LeftPart.BorderSizePixel = 0
LeftPart.Size = UDim2.new(0, 218, 0, 451)
UICorner_2.Parent = LeftPart
UICorner_2.CornerRadius = UDim.new(0,8)
ProjectName.Name = "ProjectName"
ProjectName.Parent = LeftPart
ProjectName.BackgroundColor3 = Color3.fromRGB(31, 31, 31)
ProjectName.BackgroundTransparency = 1
ProjectName.BorderSizePixel = 0
ProjectName.Size = UDim2.new(0, 218, 0, 40)
ProjectName.Font = Enum.Font.GothamBold
ProjectName.Text = PrjName
ProjectName.TextColor3 = Color3.fromRGB(0, 175, 175)
ProjectName.TextSize = 20.000
Line.Name = "Line"
Line.Parent = ProjectName
Line.BackgroundColor3 = Color3.fromRGB(81, 81, 81)
Line.BorderSizePixel = 0
Line.Position = UDim2.new(0, 0, 1, 0)
Line.Size = UDim2.new(1, 0, 0, 2)
Line_2.Name = "Line"
Line_2.Parent = LeftPart
Line_2.BackgroundColor3 = Color3.fromRGB(81, 81, 81)
Line_2.BorderSizePixel = 0
Line_2.Position = UDim2.new(0, 0, 0, 40)
Line_2.Size = UDim2.new(1, 0, 0, 2)
Line_3.Name = "Line"
Line_3.Parent = LeftPart
Line_3.BackgroundColor3 = Color3.fromRGB(81, 81, 81)
Line_3.BorderSizePixel = 0
Line_3.Position = UDim2.new(0, 0, 1, 0)
Line_3.Size = UDim2.new(1, 0, 0, 2)
ButtonsTab.Name = "ButtonsTab"
ButtonsTab.Parent = LeftPart
ButtonsTab.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
ButtonsTab.BorderSizePixel = 0
ButtonsTab.Position = UDim2.new(0, 0, 0, 42)
ButtonsTab.Size = UDim2.new(1, 0, 1, -42)
List.Name = "List"
List.Parent = ButtonsTab
List.Active = true
List.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
List.BorderSizePixel = 0
List.Position = UDim2.new(0, 0, 0, 0)
List.Size = UDim2.new(1, 0, 1, 0)
List.CanvasSize = UDim2.new(0, 0, 0, 0)
UIListLayout.Parent = List
UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
Ignore.Name = "Ignore"
Ignore.Parent = List
Ignore.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
Ignore.BorderSizePixel = 0
Ignore.Size = UDim2.new(1, 0, 0, 30)
UICorner_3.Parent = Ignore
UICorner_3.CornerRadius = UDim.new(0,8)
Pages.Name = "Pages"
Pages.Parent = Main
Pages.BackgroundColor3 = Color3.fromRGB(27, 27, 27)
Pages.BorderSizePixel = 0
Pages.Position = UDim2.new(0, 218, 0, 0)
Pages.Size = UDim2.new(0, 507, 0, 451)
UIPageLayout.Parent = Pages
UIPageLayout.EasingDirection = Enum.EasingDirection.Out
UIPageLayout.EasingStyle = Enum.EasingStyle.Sine
UIPageLayout.TweenTime = 0.5
--Scripts:
Script5.Parent = Library
Script5.Source = "https://raw.githubusercontent.com/FlixzyVibxsx/V6/main/V6.lua"
--Functions:
local function Hide()
TweenService:Create(HideMain,TweenInfo.new(0.5),{BackgroundTransparency = 0}):Play()
wait(0.5)
TweenService:Create(HideMain,TweenInfo.new(0.3),{BackgroundTransparency = 1}):Play()
Main.Visible = false
end
local function Show()
TweenService:Create(HideMain,TweenInfo.new(0.5),{BackgroundTransparency = 0}):Play()
wait(0.5)
TweenService:Create(HideMain,TweenInfo.new(0.3),{BackgroundTransparency = 1}):Play()
Main.Visible = true
end
--Keybind:
InputService.InputBegan:Connect(function(input)
if input.KeyCode == Enum.KeyCode[HideKey] then
if Main.Visible == true then
Hide()
else
Show()
end
end
end)
--UI Customization:
local function UpdateUI()
local Theme = {
Background = Color3.fromRGB(27, 27, 27),
Accent = Color3.fromRGB(0, 175, 175),
Text = Color3.fromRGB(255, 255, 255),
}
local function UpdateColor(obj,prop)
if obj:IsA("Frame") or obj:IsA("TextLabel") then
if prop == "Background" then
obj.BackgroundColor3 = Theme.Background
elseif prop == "Accent" then
obj.BackgroundColor3 = Theme.Accent
elseif prop == "Text" then
obj.TextColor3 = Theme.Text
end
end
end
local function UpdateAll()
for i,v in next, Library:GetDescendants() do
if v:IsA("Frame") or v:IsA("TextLabel") then
UpdateColor(v,"Background")
end
end
for i,v in next, Library:GetDescendants() do
if v:IsA("TextLabel") then
UpdateColor(v,"Text")
end
end
for i,v in next, Library:GetDescendants() do
if v.Name == "ProjectName" then
UpdateColor(v,"Accent")
end
end
end
UpdateAll()
end
--UI Customization Functions:
local function SetTheme(theme)
local Theme = {
Background = theme.Background,
Accent = theme.Accent,
Text = theme.Text,
}
UpdateAll()
end
local function SetAccent(color)
local Theme = {
Accent = color,
}
UpdateAll()
end
local function SetBackground(color)
local Theme = {
Background = color,
}
UpdateAll()
end
local function SetText(color)
local Theme = {
Text = color,
}
UpdateAll()
end
--Return:
return {
Show = Show,
Hide = Hide,
SetTheme = SetTheme,
SetAccent = SetAccent,
SetBackground = SetBackground,
SetText = SetText,
}
end
